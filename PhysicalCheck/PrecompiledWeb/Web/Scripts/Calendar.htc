		   else
		   document.all("timeContain").style.display="none";
		   value = new Date();
		   fillTime();
		  
		}
      
       
    }

   



    var nYear = value.getYear();
    if (nYear < 100) {
        nYear += 1900;
    }
    if ((nYear >= c_nMinYear) && (nYear <= c_nMaxYear)) {
        g_nYear = nYear;
        g_nMonth = value.getMonth() + 1;
        g_nDay = value.getDate();

        InitializeLeapYear();

        if (g_fInitialized == true) {
