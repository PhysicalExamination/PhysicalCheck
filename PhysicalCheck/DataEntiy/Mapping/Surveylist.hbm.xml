<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DataEntity.Survey" assembly="DataEntity">
    <class name="SurveyListEntity" table="surveylist" >
        <id name="SID" column="SID" type="Int32" >
            <generator class="assigned" />
        </id>
        <property name="SurveyName" column="SurveyName" type="String" not-null="true" length="300" /> 
        <property name="Sex" column="Sex" type="String" not-null="true" length="9" /> 
        <property name="Disease" column="Disease" type="String" not-null="false" length="900" /> 
        <property name="Enabled" column="Enabled" type="Boolean" not-null="true" /> 
        <property name="Description" column="Description" type="String" not-null="false" length="600" /> 
    </class>
</hibernate-mapping>

