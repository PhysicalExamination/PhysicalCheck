<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DataEntity.Admin" assembly="DataEntity">
    <class name="SysUserEntity" table="sysuser" >
        <id name="UserNo" column="UserNo" type="String" length="60" >
            <generator class="assigned" />
        </id>
        <property name="DeptNo" column="DeptNo" type="Int32" not-null="false" /> 
        <property name="UserName" column="UserName" type="String" not-null="false" length="90" /> 
        <property name="Position" column="Position" type="String" not-null="false" length="150" /> 
        <property name="LinkTel" column="LinkTel" type="String" not-null="false" length="60" /> 
        <property name="Mobile" column="Mobile" type="String" not-null="false" length="60" /> 
        <!--<property name="UserRegion" column="UserRegion" type="String" not-null="false" length="30" />--> 
        <property name="UserCategory" column="UserCategory" type="String" not-null="false" length="3" /> 
        <property name="LoginIP" column="LoginIP" type="String" not-null="false" length="120" /> 
        <property name="IsBindLogin" column="IsBindLogin" type="Byte" not-null="false" /> 
        <property name="PassWord" column="PassWord" type="String" not-null="false" length="450" /> 
        <property name="OrderNo" column="OrderNo" type="Int32" not-null="false" /> 
        <property name="UserAccount" column="UserAccount" type="String" not-null="true" length="150" /> 
        <!--<property name="WorkDept" column="WorkDept" type="String" not-null="false" length="65535" />--> 
    </class>
</hibernate-mapping>

