            <generator class="assigned" />
        </id>
        <property name="RegionName" column="RegionName" type="String" not-null="false" length="150" /> 
        <property name="ParentNode" column="ParentNode" type="String" not-null="true" length="30" /> 
        <property name="Comment" column="Comment" type="String" not-null="false" length="150" /> 
        <property name="Spell" column="Spell" type="String" not-null="false" length="60" /> 
    </class>
</hibernate-mapping>

