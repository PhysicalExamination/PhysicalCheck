<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DataEntity.Survey" assembly="DataEntity">
    <class name="QuestionOptionEntity" table="questionoption" >
		<composite-id name="ID" class="QuestionOptionPK">
			<key-property name="QID" column="QID" type="Int32" />
			<key-property name="SN" column="SN" type="Int32" />
		</composite-id>
        <property name="OptionTitle" column="OptionTitle" type="String" not-null="false" length="900" /> 
        <property name="OptionValue" column="OptionValue" type="Int32" not-null="false" /> 
    </class>
</hibernate-mapping>

