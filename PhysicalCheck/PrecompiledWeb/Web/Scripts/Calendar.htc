		   }
		   
		  if(indexMin!=-1)	
          {    
             mi = value.substr(indexMin,2);     
			 document.all("dpMinute").value = mi;
		   }
		   else
		   document.all("dpMinute").value = "00";
		   
		  if(indexSen!=-1)	
