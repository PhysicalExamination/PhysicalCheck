<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DataEntity.Survey" assembly="DataEntity">
    <class name="EvaluateFactorEntity" table="evaluatefactor" >
        <id name="EID" column="EID" type="Int32" >
            <generator class="assigned" />
        </id>
        <property name="FactorName" column="FactorName" type="String" not-null="false" length="300" /> 
        <property name="FactorRate" column="FactorRate" type="Double" not-null="false" /> 
        <property name="CriticalValue" column="CriticalValue" type="Int32" not-null="false" /> 
        <property name="Medicine" column="Medicine" type="String" not-null="false" length="900" /> 
        <property name="Suggestion" column="Suggestion" type="String" not-null="false" length="900" /> 
        <property name="Enabled" column="Enabled" type="Boolean" not-null="true" /> 
    </class>
</hibernate-mapping>

